{
  "name": "seminar-2024-frontend-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "vite",
    "style": "npx tailwindcss -i src/App.css -o src/output.css --watch",
    "build": "tsc -b && vite build",
    "types:check": "tsc",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "unused:check": "knip",
    "check-all": "yarn types:check && yarn format:check && yarn lint:check && yarn unused:check"
  },
  "dependencies": {
    "@eslint/js": "9.16.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/testing-library__react-hooks": "4.0.0",
    "@vitejs/plugin-react": "4.3.4",
    "jsdom": "25.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@woohm402/eslint-config-react": "0.6.4",
    "autoprefixer": "10.4.20",
    "eslint": "9.10.0",
    "knip": "5.30.2",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.12",
    "typescript": "5.6.2",
    "vite": "5.4.5",
    "vitest": "2.1.7"
  },
  "packageManager": "yarn@4.4.1"
}
